{
  "name": "node-red-contrib-xsltransform",
  "version": "0.5.24",
  "description": "Node-RED node node to tranform xml content using saxon-js.",
  "main": "xsltransform.js",
  "dependencies": {
    "node-red-contrib-logger": "*",
    "saxon-js": "*",
    "xslt3": "*",
    "cross-spawn":"*"
  },
  "devDependencies": {
    "axios": ">=0.21.2",
    "bcrypt": ">=5.0.0",
    "mocha": ">=9.1.3",
    "node-red": "^3.0.2",
    "node-red-node-test-helper": "*",
    "nth-check": ">=2.0.1",
    "ws": ">=6.2.2"
  },
  "scripts": {
    "test": "mocha \"test/n*.js\"",
    "startNodeRed": "node-red --userDir ./.temp --title '***test***' --setting ./test/data/settings.js ./test/data/flow.json",
    "link": "cd ./.temp ; npm link ../",
    "update":"npm update"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/peterprib/node-red-contrib-xsltransform.git"
  },
  "keywords": [
    "node-red",
    "saxon",
    "transform",
    "xml",
    "xsl",
    "xslt",
    "xslt3.0"
  ],
  "node-red": {
    "nodes": {
      "xsltransform": "xsltransform/xsltransform.js"
    }
  },
  "author": "Peter Prib",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/peterprib/node-red-contrib-xsltransform/issues"
  },
  "homepage": "https://github.com/peterprib/node-red-contrib-xsltransform#readme"
}
